{% extends'base.html.twig' %}
{% block body %}
    <article>
        <h2>{{ article.title }}</h2>
        <div class="metadata">Ecrit le {{article.createdAt | date('d/m/Y') }}
            à {{ article.createdAt | date('H:i') }} catégorie A</div>
        <div class="content">
            {{ article.description | raw }}
    </article>
{% endblock body %}